      </Aliases>
    </Structure>
    <Structure>
      <Name>Dose105[%]-PTV</Name>
      <Aliases>
        <Alias>D105%-PTV</Alias>
        <Alias>105-PTV</Alias>
        <Alias>Dose 105%-PTV</Alias>
      </Aliases>
    </Structure>
    <Structure>
      <Name>IGTV</Name>
      <Aliases>
        <Alias>IGTV 2</Alias>
